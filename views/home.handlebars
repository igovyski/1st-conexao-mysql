<main>
    <h2>Home</h2>

    {{#each books}}
    <div class="book">
        <h3>Título: {{this.title}}</h3>
        <p>Quantidade de páginas: {{this.pageqty}}</p>
        <a href="/book/{{this.id}}">Ver mais</a>
        <a href="/update/{{this.id}}">Editar</a>
        <a href="/delete/{{this.id}}">Deletar</a>
    </div>
    {{/each}}
</main>